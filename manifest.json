{
	"manifest_version": 2,
	"name": "ZenPage",
	"description": "A personalised homepage featuring beautiful photography, categorised bookmarks and quick links to popular sites.",
	"version": "2.0",
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"options_page": "options.html",
	"permissions": [
		"storage",
		"geolocation"
	],
	"icons": {
		"16": "./icons/icon-16.png",
		"48": "./icons/icon-48.png",
		"128": "./icons/icon-128.png"
	},
	"applications": {
		"gecko": {
			"id": "club.tilde.pedrocx486.zenpage"
		}
	},
	"content_security_policy": "script-src 'self' object-src"
}